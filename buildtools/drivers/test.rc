function execute_stage() {
  local _test_filter=${1:-.*}
  export CMD_UNIT_CONFDIR="${TESTDIR}/.cmd_unit"

  "${EXEC_BASEDIR}/bin/command-unit" \
    --parallel \
    --filter="${_test_filter}" \
    --test-report="${TESTREPORTDIR}" \
    --test-srcdir="${TESTDIR}" \
    --test-workdir="${TESTDIR}"
}
